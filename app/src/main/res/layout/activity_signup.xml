<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity"
        android:background="@android:color/white"
        android:textColor="@android:color/black"
>
    <ImageView
            android:layout_width="463dp"
            android:layout_height="680dp" app:srcCompat="@drawable/path8" android:id="@+id/imageView3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginBottom="16dp"/>
    <ImageView
            android:layout_width="181dp"
            android:layout_height="120dp" app:srcCompat="@drawable/logo" android:id="@+id/imageView4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="@+id/imageView3"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.761"
            app:layout_constraintVertical_bias="0.026"/>
    <EditText
            android:hint="First Name"
            android:layout_width="129dp"
            android:layout_height="55dp"
            android:inputType="textPersonName|textCapWords"
            android:textColor="@color/white"
            android:ems="10"
            android:id="@+id/FirstName"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/imageView3" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.311"/>
    <EditText
            android:hint="Last Name"
            android:layout_width="129dp"
            android:layout_height="55dp"
            android:inputType="textPersonName|textCapWords"
            android:textColor="@color/white"
            android:ems="10"
            android:id="@+id/LastName"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/imageView3" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.421"/>
    <EditText
            android:hint="Email"
            android:layout_width="129dp"
            android:layout_height="55dp"
            android:inputType="textPersonName"
            android:textColor="@color/white"
            android:ems="10"
            android:id="@+id/PersonalEmail"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/imageView3" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.53"/>
    <EditText
            android:hint="Password"
            android:layout_width="129dp"
            android:layout_height="55dp"
            android:inputType="textPassword"
            android:textColor="@color/white"
            android:ems="10"
            android:id="@+id/PersonalPassword"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/imageView3" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.753"/>
    <EditText
            android:hint="Confirm Pass"
            android:layout_width="129dp"
            android:layout_height="55dp"
            android:inputType="textPassword"
            android:textColor="@color/white"
            android:ems="10"
            android:id="@+id/ConfirmPersonalPassword"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/imageView3" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.872"/>
    <Spinner
            android:layout_width="196dp"
            android:layout_height="40dp" android:id="@+id/spinner"
            android:entries="@array/spinner_items"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/imageView3" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.639" android:minHeight="48dp"/>
    <Button
            android:text="Sign Up"
            android:layout_width="98dp"
            android:layout_height="48dp" android:id="@+id/signupButton"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.051" app:layout_constraintVertical_bias="0.949"/>


</androidx.constraintlayout.widget.ConstraintLayout>